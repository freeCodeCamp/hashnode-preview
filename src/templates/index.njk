    {# START: block seo #}
    {% if post.brief %}
      <meta name="description" content="{{ post.brief }}"/>
    {% endif %}

    {# Facebook OpenGraph #}
    <meta property="og:site_name" content="freeCodeCamp.org"/>
    <meta property="og:type" content="article"/>
    <meta property="og:title" content="{{ post.title }}"/>
    {% if post.brief %}
      <meta property="og:description" content="{{ post.brief }}"/>
    {% endif %}
    <meta
      property="og:url"
      content="www.freecodecamp.org/news/{{ post.slug }}/"
/>
    <meta property="og:image" content="{{ postCoverImageURL }}"/>
    <meta property="article:published_time" content="{{ post.publishedAt }}"/>
    <meta
      property="article:modified_time"
      content="{{ post.updatedAt if post.updatedAt else post.publishedAt }}"
/>
    {% for tag in post.tags %}
      <meta property="article:tag" content="{{ tag.name }}"/>
    {% endfor %}
    <meta
      property="article:publisher"
      content="https://www.facebook.com/freecodecamp"
/>
    {% if post.author.socialMediaLinks.facebook %}
      <meta
        property="article:author"
        content="{{ post.author.socialMediaLinks.facebook }}"
/>
    {% endif %}

    {# Twitter Card #}
    <meta name="twitter:card" content="summary_large_image"/>
    <meta name="twitter:title" content="{{ post.title }}"/>
    {% if post.brief %}
      <meta name="twitter:description" content="{{ post.brief }}"/>
    {% endif %}
